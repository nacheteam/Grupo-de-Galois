       RowBox[{"Divide", "[", 
        RowBox[{"f", ",", "g1"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t\t\t\t", 
      
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"IrreduciblePolynomialQ", "[", 
