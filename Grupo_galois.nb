Cell["Comprueba si el polinomio tiene una ra\[IAcute]z de grado 1.", "Text",
 CellChangeTimes->{{3.7244354977376328`*^9, 3.724435533303047*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ComprobarUnaRaizGrado5", "[", 
    RowBox[{"f_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"factores", ",", "condicion"}], "}"}], ",", 
     RowBox[{
      RowBox[{"factores", "=", 
       RowBox[{"FactorList", "[", "f", "]"}]}], ";", "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"condicion", "=", "False"}], ";", "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Exponent", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"factores", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "[", 
              RowBox[{"[", "1", "]"}], "]"}], ",", "x"}], "]"}], "\[Equal]", 
           "1"}], ",", 
          RowBox[{"condicion", "=", "True"}], ","}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "factores", "]"}]}], "}"}]}], "]"}], ";", 
      "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"Return", "[", "condicion", "]"}]}]}], "]"}]}], ";"}]], "Code",
 CellChangeTimes->{{3.723118871903914*^9, 3.7231190122112303`*^9}, {
