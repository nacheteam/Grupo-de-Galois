          SuperscriptBox["x", "2"], "-", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Discriminant", "[", 
             RowBox[{"g1", ",", "x"}], "]"}], "*", 
            RowBox[{"Discriminant", "[", 
             RowBox[{"g2", ",", "x"}], "]"}]}], ")"}]}], "]"}], ",", 
        RowBox[{"Return", "[", "False", "]"}], ",", 
        RowBox[{"Return", "[", "True", "]"}]}], "]"}]}]}], "]"}]}], 
  ";"}]], "Code",
