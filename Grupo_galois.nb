Cell["Funcion que devuelve la resolvente cubica.", "Text",
 CellChangeTimes->{{3.724434989609294*^9, 3.7244349987447557`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ResolventeCubica", "[", 
    RowBox[{"f_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"coefs", ",", "b0", ",", "b1", ",", "b2"}], "}"}], ",", 
     RowBox[{
      RowBox[{"coefs", "=", 
       RowBox[{"CoefficientList", "[", 
        RowBox[{"f", ",", "x"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"b2", "=", 
       RowBox[{"-", 
        RowBox[{"coefs", "[", 
         RowBox[{"[", "3", "]"}], "]"}]}]}], ";", "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"b1", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"coefs", "[", 
          RowBox[{"[", "2", "]"}], "]"}], "*", 
         RowBox[{"coefs", "[", 
          RowBox[{"[", "4", "]"}], "]"}]}], "-", 
        RowBox[{"4", "*", 
         RowBox[{"coefs", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}]}]}], ";", "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"b0", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox[
           RowBox[{"coefs", "[", 
            RowBox[{"[", "4", "]"}], "]"}], "2"]}], "*", 
         RowBox[{"coefs", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], "+", 
        RowBox[{"4", "*", 
         RowBox[{"coefs", "[", 
          RowBox[{"[", "3", "]"}], "]"}], "*", 
         RowBox[{"coefs", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], "-", 
        SuperscriptBox[
         RowBox[{"coefs", "[", 
          RowBox[{"[", "2", "]"}], "]"}], "2"]}]}], ";", "\n", 
      "\t\t\t\t\t\t\t", 
      RowBox[{"Return", "[", 
       RowBox[{
        SuperscriptBox["x", "3"], "+", 
        RowBox[{"b2", "*", 
         SuperscriptBox["x", "2"]}], "+", 
        RowBox[{"b1", "*", "x"}], "+", "b0"}], "]"}]}]}], "]"}]}], 
  ";"}]], "Code",
 CellChangeTimes->{{3.722711484509028*^9, 3.7227115421283627`*^9}, {
  3.722711572502029*^9, 3.722711616467927*^9}, {3.722711678850979*^9, 
  3.722711704523776*^9}, {3.7227117513181763`*^9, 3.7227119572172117`*^9}, {
  3.722712978189889*^9, 3.722712982151016*^9}}],

