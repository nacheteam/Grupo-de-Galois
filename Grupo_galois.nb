Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FactoresFormateados", "[", 
    RowBox[{"f_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"lista", ",", "factores"}], "}"}], ",", 
     RowBox[{
      RowBox[{"factores", "=", 
       RowBox[{"FactorList", "[", 
        RowBox[{"f", ",", "k"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"lista", "=", 
       RowBox[{"List", "[", "]"}]}], ";", "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"lista", ",", 
          RowBox[{"Expand", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"factores", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}], "^", 
            RowBox[{
             RowBox[{"factores", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "factores", "]"}]}], "}"}]}], "]"}], ";", 
      "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"SortBy", "[", 
       RowBox[{"lista", ",", 
        RowBox[{
         RowBox[{"Exponent", "[", 
          RowBox[{"#1", ",", "x"}], "]"}], "<", 
         RowBox[{"Exponent", "[", 
          RowBox[{"#2", ",", "x"}], "]"}]}]}], "]"}], ";", "\n", 
      "\t\t\t\t\t\t\t", 
      RowBox[{"Return", "[", "lista", "]"}]}]}], "]"}]}], ";"}]], "Code",
