Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ObtenerBetasSoluciones", "[", 
    RowBox[{"f_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "soluciones", ",", "solucionesFormateadas", ",", "beta1", ",", "beta2", 
       ",", "beta3"}], "}"}], ",", 
     RowBox[{
      RowBox[{"soluciones", "=", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"f", "\[Equal]", "0"}], ",", "x"}], "]"}]}], ";", "\n", 
      "\t\t\t\t\t\t\t\t", 
      RowBox[{"solucionesFormateadas", "=", 
       RowBox[{"List", "[", "]"}]}], ";", "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"solucionesFormateadas", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"soluciones", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "1", "]"}], "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "soluciones", "]"}]}], "}"}]}], "]"}], ";", 
      "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"beta1", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "*", 
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}], " ", "+", " ", 
        RowBox[{
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "3", "]"}], "]"}], "*", 
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "4", "]"}], "]"}]}]}]}], ";", "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"beta2", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "*", 
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "3", "]"}], "]"}]}], " ", "+", " ", 
        RowBox[{
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "2", "]"}], "]"}], "*", 
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "4", "]"}], "]"}]}]}]}], ";", "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"beta3", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "*", 
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "4", "]"}], "]"}]}], " ", "+", " ", 
        RowBox[{
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "2", "]"}], "]"}], "*", 
         RowBox[{"solucionesFormateadas", "[", 
          RowBox[{"[", "3", "]"}], "]"}]}]}]}], ";", "\n", "\t\t\t\t\t\t\t\t", 
      RowBox[{"Return", "[", 
       RowBox[{"List", "[", 
        RowBox[{
         RowBox[{"List", "[", 
          RowBox[{"beta1", ",", "beta2", ",", "beta3"}], "]"}], ",", 
         "solucionesFormateadas"}], "]"}], "]"}]}]}], "]"}]}], ";"}]], "Code",\

