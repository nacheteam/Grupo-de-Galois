Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ObtenerFactorBueno2General", "[", 
    RowBox[{"f_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"factores", ",", 
       RowBox[{"mayor", "=", "1"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"factores", "=", 
       RowBox[{"FactorList", "[", 
        RowBox[{"f", ",", "k"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
