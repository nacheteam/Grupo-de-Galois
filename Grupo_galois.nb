 CellChangeTimes->{{3.72443505499899*^9, 3.724435092753255*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CompruebaDiscriminantes", "[", 
    RowBox[{"f_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"g1", ",", "g2"}], "}"}], ",", 
     RowBox[{
      RowBox[{"g1", "=", 
       RowBox[{"DevuelvePolinomioGrado2", "[", 
        RowBox[{"f", ",", "x"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t\t\t\t", 
