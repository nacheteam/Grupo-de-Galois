Cell["\<\
Comprueba la condici\[OAcute]n de los alphas y betas con la extensi\[OAcute]n \
por la raiz del discriminante.\
\>", "Text",
 CellChangeTimes->{{3.724435443405669*^9, 3.7244354720226173`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ComprobarBetasAlphas", "[", 
    RowBox[{"f_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "betas", ",", "alphas", ",", "discriminante", ",", "condicion"}], "}"}],
      ",", 
     RowBox[{
      RowBox[{"betas", "=", 
       RowBox[{
        RowBox[{"ObtenerBetasSoluciones", "[", 
         RowBox[{"f", ",", "x"}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"alphas", "=", 
       RowBox[{
        RowBox[{"ObtenerBetasSoluciones", "[", 
         RowBox[{"f", ",", "x"}], "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"discriminante", "=", 
       RowBox[{"Discriminant", "[", 
        RowBox[{"f", ",", "x"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"condicion", "=", "True"}], ";", "\n", "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"IrreduciblePolynomialQ", "[", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"alphas", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "+", 
             RowBox[{"alphas", "[", 
              RowBox[{"[", "3", "]"}], "]"}]}], ")"}]}], ",", 
          RowBox[{"Extension", "\[Rule]", 
           RowBox[{"Sqrt", "[", "discriminante", "]"}]}]}], "]"}], ",", 
        RowBox[{"condicion", "=", "False"}], ","}], "]"}], ";", "\n", 
      "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"IrreduciblePolynomialQ", "[", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"alphas", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "*", 
             RowBox[{"alphas", "[", 
              RowBox[{"[", "3", "]"}], "]"}]}], ")"}]}], ",", 
          RowBox[{"Extension", "\[Rule]", 
           RowBox[{"Sqrt", "[", "discriminante", "]"}]}]}], "]"}], ",", 
        RowBox[{"condicion", "=", "False"}], ","}], "]"}], ";", "\n", 
      "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"IrreduciblePolynomialQ", "[", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"alphas", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "+", 
             RowBox[{"alphas", "[", 
              RowBox[{"[", "4", "]"}], "]"}]}], ")"}]}], ",", 
          RowBox[{"Extension", "\[Rule]", 
           RowBox[{"Sqrt", "[", "discriminante", "]"}]}]}], "]"}], ",", 
        RowBox[{"condicion", "=", "False"}], ","}], "]"}], ";", "\n", 
      "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"IrreduciblePolynomialQ", "[", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"alphas", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "*", 
             RowBox[{"alphas", "[", 
              RowBox[{"[", "4", "]"}], "]"}]}], ")"}]}], ",", 
          RowBox[{"Extension", "\[Rule]", 
           RowBox[{"Sqrt", "[", "discriminante", "]"}]}]}], "]"}], ",", 
        RowBox[{"condicion", "=", "False"}], ","}], "]"}], ";", "\n", 
      "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"Return", "[", "condicion", "]"}]}]}], "]"}]}], ";"}]], "Code",
