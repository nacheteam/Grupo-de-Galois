Cell["C\[AAcute]lculo del grupo de Galois de un polinomio de grado 4.", "Text",
 CellChangeTimes->{{3.724435475333078*^9, 3.724435486603057*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"GrupoGalois4", "[", 
    RowBox[{"f_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"CompruebaDescomposicionGrado3", "[", 
        RowBox[{"f", ",", "x"}], "]"}], ",", "\n", "\t\t\t\t\t\t\t\t\t", 
       RowBox[{"GrupoGalois3", "[", 
        RowBox[{"DevuelvePolinomioGrado3", "[", 
         RowBox[{"f", ",", "x"}], "]"}], "]"}], ",", "\n", 
       "\t\t\t\t\t\t\t\t\t", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"CompruebaDescomposicionGrado2", "[", 
          RowBox[{"f", ",", "x"}], "]"}], ",", "\n", "\t\t\t\t\t\t\t\t\t\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"CompruebaDiscriminantes", "[", 
            RowBox[{"f", ",", "x"}], "]"}], ",", "\n", 
           "\t\t\t\t\t\t\t\t\t\t\t", 
           RowBox[{
           "Print", "[", "\"\<El grupo de Galois es <(12)(34)>\>\"", "]"}], 
           ",", "\n", "\t\t\t\t\t\t\t\t\t\t\t", 
           RowBox[{
           "Print", "[", "\"\<El grupo de Galois es <(12),(34)>\>\"", "]"}]}],
           "]"}], ",", "\n", "\t\t\t\t\t\t\t\t\t\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"CompruebaResolventeIrreducible", "[", 
            RowBox[{"f", ",", "x"}], "]"}], ",", "\n", 
           "\t\t\t\t\t\t\t\t\t\t\t", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"!", 
              RowBox[{"CompruebaDiscriminanteResolvente", "[", 
               RowBox[{"f", ",", "x"}], "]"}]}], ",", "\n", 
             "\t\t\t\t\t\t\t\t\t\t\t\t", 
             RowBox[{
             "Print", "[", 
              "\"\<El grupo de Galois de f es S4 y el de la resolvente \
S3\>\"", "]"}], ",", "\n", "\t\t\t\t\t\t\t\t\t\t\t\t", 
             RowBox[{
             "Print", "[", 
              "\"\<El grupo de Galois de f es A4 y el de la resolvente \
S4\>\"", "]"}]}], "]"}], ",", "\n", "\t\t\t\t\t\t\t\t\t\t\t", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"CompruebaTresRaicesResolvente", "[", 
              RowBox[{"f", ",", "x"}], "]"}], ",", "\n", 
             "\t\t\t\t\t\t\t\t\t\t\t\t", 
             RowBox[{
             "Print", "[", 
              "\"\<El grupo de Galois de f es V = <(12)(34),(13)(24)> y el de \
la resolvente es el trivial.\>\"", "]"}], ",", "\n", 
             "\t\t\t\t\t\t\t\t\t\t\t\t", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"ComprobarBetasAlphas", "[", 
                RowBox[{"f", ",", "x"}], "]"}], ",", "\n", 
               "\t\t\t\t\t\t\t\t\t\t\t\t\t", 
               RowBox[{
               "Print", "[", 
                "\"\<El grupo de Galois de f es C4 y el de la resolvente es \
C2\>\"", "]"}], ",", "\n", "\t\t\t\t\t\t\t\t\t\t\t\t\t", 
               RowBox[{
               "Print", "[", 
                "\"\<El grupo de Galois de f es D4 y el de la resolvente es \
C2\>\"", "]"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}]}], 
  ";"}]], "Code",
