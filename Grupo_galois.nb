Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ObtenerFactorBueno2", "[", 
    RowBox[{"f_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"factores", ",", 
       RowBox[{"mayor", "=", "1"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"factores", "=", 
       RowBox[{"FactorList", "[", 
        RowBox[{"f", ",", "k"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"factores", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}], "\[NotEqual]", "1"}], " ", 
           "\[And]", " ", 
           RowBox[{
            RowBox[{
             RowBox[{"factores", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}], ">", 
            RowBox[{
             RowBox[{"factores", "[", 
              RowBox[{"[", "mayor", "]"}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}]}]}], ",", 
          RowBox[{"mayor", "=", "i"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "factores", "]"}]}], "}"}]}], "]"}], ";", 
      "\n", "\t\t\t\t\t\t\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"factores", "[", 
           RowBox[{"[", "mayor", "]"}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}], "\[Equal]", "3"}], ",", "\n", 
        "\t\t\t\t\t\t\t\t", 
        RowBox[{"Return", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"factores", "[", 
            RowBox[{"[", "mayor", "]"}], "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}], "^", "2"}], "]"}], ",", "\n", 
        "\t\t\t\t\t\t\t\t", 
        RowBox[{"Return", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"factores", "[", 
            RowBox[{"[", "mayor", "]"}], "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}], "^", 
          RowBox[{
           RowBox[{"factores", "[", 
            RowBox[{"[", "mayor", "]"}], "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], "]"}]}]}], "]"}]}], 
