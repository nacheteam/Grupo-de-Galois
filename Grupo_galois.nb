Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DevuelveFactorGrado4", "[", 
    RowBox[{"f_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"factores", ",", "factorGrado1", ",", "factorGrado4"}], "}"}], 
     ",", 
     RowBox[{
      RowBox[{"factores", "=", 
       RowBox[{"FactorList", "[", "f", "]"}]}], ";", "\n", 
      "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Exponent", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"factores", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "[", 
              RowBox[{"[", "1", "]"}], "]"}], ",", "x"}], "]"}], "\[Equal]", 
           "1"}], ",", 
          RowBox[{"factorGrado1", "=", 
           RowBox[{
            RowBox[{"factores", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], ","}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "factores", "]"}]}], "}"}]}], "]"}], ";", 
      "\n", "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"factorGrado4", "=", 
       RowBox[{"Divide", "[", 
        RowBox[{"f", ",", "factorGrado1"}], "]"}]}], ";", "\n", 
      "\t\t\t\t\t\t\t\t\t", 
      RowBox[{"Return", "[", "factorGrado4", "]"}]}]}], "]"}]}], 
  ";"}]], "Code",
 CellChangeTimes->{{3.723119112815178*^9, 3.723119255069504*^9}}],

